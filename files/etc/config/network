
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth0'

config interface 'lan'
	option device 'eth0'
	option proto 'static'
	option ipaddr '192.168.27.5'
	option netmask '255.255.255.0'
	option gateway '192.168.27.1'
	list dns '192.168.27.1'
	option delegate '0'

